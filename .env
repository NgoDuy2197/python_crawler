# Base URL to start crawling from
START_URL=https://baomoi.com/

# CSS selector for pager (next page button/link)
PAGER_SELECTOR=.next-page

# Maximum number of pages to crawl (0 for no limit)
MAX_PAGES=100

# Timeout for page loading in seconds
PAGE_LOAD_TIMEOUT=30

# User agent to use for requests
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36
